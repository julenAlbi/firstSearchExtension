{
    "manifest_version": 3,
    "name": "First Google Search Result",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["*://www.google.com/*"],
            "js": ["content.js"]
        }
    ],
    "commands": {
        "open-first-result": {
            "suggested_key": {
                "default": "Ctrl+A",
                "mac": "Command+A"
            },
            "description": "Open the first Google search result"
        }
    },
	"omnibox": {
        "keyword": "fs"
    }
}